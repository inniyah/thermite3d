material BrickMaterial : ColourMapAndNormalMap3D
{
	technique
	{
		pass
		{
		        scene_blend alpha_blend
			texture_unit
			{
				texture BrickColourMap.dds
			}

			texture_unit
			{
				texture BrickNormalMap.dds
			}
		}
	}
	
	technique
	{
	         pass
	         {
	             ambient 0.3 0.0 0.0
	             diffuse 1.0 0.0 0.0
	         }
	}
}